@model Library.Models.Account.RegisterOperModel
@{
    ViewBag.Title = "Регистрация нового оператора";
}
<div class="row">
    <div class="col-md-4 col-md-offset-4">
        @using (Html.BeginForm())
        {
            <form class="form-horizontal">
                <fieldset>
                    <legend>@ViewBag.Title</legend>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.Fio1, new { @class = "form-control", placeholder = "ФИО", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.Birthday1, new { @class = "form-control", placeholder = "Дата рождения ДД/ММ/ГГГГ", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.PassportSeries1, new { @class = "form-control", placeholder = "Паспорт №1111", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.PassportKod1, new { @class = "form-control", placeholder = "Серия 111 111", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.CertificateCode1, new { @class = "form-control", placeholder = "Номер свид. о рождении AA-11111", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.Email1, new { @class = "form-control", placeholder = "Email", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.Login1, new { @class = "form-control", placeholder = "Логин", type = "text" })
                    </div>
                    <div class="form-group">
                        @Html.TextBoxFor(x => x.Password1, new { @class = "form-control", placeholder = "Пароль", type = "password" })
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-primary btn-block">Зарегистрировать оператора</button>
                    </div>
                </fieldset>
            </form>
        }
    </div>
</div>
